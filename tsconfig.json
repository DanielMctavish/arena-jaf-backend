{
  "compilerOptions": {
    /* Language and Environment */
    "resolveJsonModule": true,
    "target": "es2016",
    /* Modules */
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    /* Completeness */
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}